{
  "version": 2,
  "builds": [
    {
      "src": "logto_auth/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "logto_auth/main.py"
    }
  ],
  "env": {
    "APP_MODULE": "logto_auth:main"
  },
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  }
}
