{
  "name": "@alphab/db-scripts",
  "version": "0.1.0",
  "description": "Database CLI tools for migration, seeding, and type generation",
  "private": true,
  "type": "module",
  "bin": {
    "db": "./bin/alphab-migrate",
    "alphab-migrate": "./bin/alphab-migrate"
  },
  "files": [
    "bin/**",
    "dist/**"
  ],
  "scripts": {
    "clean": "alphab-clean",
    "bootstrap": "alphab-bootstrap",
